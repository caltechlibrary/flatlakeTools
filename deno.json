{
  "tasks": {
    "build": "deno compile --allow-read --output bin/flt mod.ts",
    "test": "deno test --allow-read",
    "dist_linux_x86_64": "deno task dist_linux_x86_64_flt",
    "dist_linux_x86_64_flt": "deno compile   --allow-read --output dist/bin/flt --target x86_64-unknown-linux-gnu mod.ts",
    "dist_linux_aarch64": "deno task dist_linux_aarch64_flt ",
    "dist_linux_aarch64_flt": "deno compile  --allow-read --output dist/bin/flt --target aarch64-unknown-linux-gnu mod.ts",
    "dist_macos_x86_64": "deno task dist_macos_x86_64_flt ",
    "dist_macos_x86_64_flt": "deno compile   --allow-read --output dist/bin/flt --target x86_64-apple-darwin mod.ts",
    "dist_macos_aarch64": "deno task dist_macos_aarch64_flt ",
    "dist_macos_aarch64_flt": "deno compile  --allow-read --output dist/bin/flt --target aarch64-apple-darwin mod.ts",
    "dist_windows_x86_64": "deno task dist_windows_x86_64_flt ",
    "dist_windows_x86_64_flt": "deno compile --allow-read --output dist/bin/flt.exe --target x86_64-pc-windows-msvc mod.ts",
    "dist_windows_aarch64": "deno task dist_windows_aarch64_flt ",
    "dist_windows_aarch64_flt": "deno compile --allow-read --output dist/bin/flt.exe --target aarch64-pc-windows-msvc mod.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/yaml": "jsr:@std/yaml@^1.0.9"
  }
}
